net: "/path/to/alexnet_cifar100/Alexnet_cifar100_train_val.prototxt"
test_iter: 100
test_interval: 1000
base_lr: 0.01

lr_policy: "inv"
# lr_policy: "step"
# stepsize: 100000
gamma: 0.1
power: 0.2

display: 100
max_iter: 20000
momentum: 0.7
weight_decay: 0.0005
snapshot: 10000
snapshot_prefix: "/path/to/alexnet_cifar100/Alexnet_cifar100_solver" # Alexnet_cifar100_solver_iter_XXXXX.caffemodel

solver_mode: GPU # GPU Mode
